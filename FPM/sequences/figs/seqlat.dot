	empty--C;
	empty--G;
	empty--T;
	A -- AA;
	A -- AG;
	A -- AT;
	G -- GA;
	G -- GG;
	G -- GT;
	T -- TA;
	T -- TG;
	T -- TT;
	AA -- AAA;
	AA -- AAG;
	AG -- AGA;
	AG -- AGG;
